#include <iostream>

using namespace std;

int main()
{
	int r,c,i;
	
	cout << "How many rows: ";
	cin >> r;
	
	cout << "How many columns: ";
	cin >> c;
			
		for (int i=1;i<=r;i++)
		    {   	
  				for (int i=1;i<=c;i++)
    				{
 		 	 		   cout <<"*";
			   		}
				  cout <<endl;  	
			  }	
				
	return 0;			
}
